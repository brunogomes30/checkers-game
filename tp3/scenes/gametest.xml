<sxs>
    <scene root="demoRoot" axis_length="5" />

    <views default="boardCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="0" y="2.5" z="2.5" />
            <to x="0" y="0" z="0" />
        </perspective>
        <ortho id="orthoCam" near="-5" far="5" left="-5" right="5" bottom="-5" top="5">
            <from x="0" y="0" z="3" />
            <to x="0" y="0" z="0" />
        </ortho>
        <perspective id="boardCamera" near="0.1" far="500" angle="45" class="board-camera">
            <from x="0" y="1.5" z="2.0" />
            <to x="0" y="0" z="0" />
        </perspective>
    </views>

    <ambient>
        <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
        <background r="0.3" g="0.3" b="0.3" a="1.0" />
    </ambient>

    <lights>
        <omni id="demoLight" enabled="1" exponent="127">
            <location x="0" y="2" z="5" w="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.3" g="0.3" b="0.3" a="1.0" />
            <specular r="0.2" g="0.2" b="0.2" a="1.0" />
            <attenuation constant="0.0" linear="0.05" quadratic="0.0" />
        </omni>
        <omni id="demoLight2" enabled="1" exponent="127">
            <location x="0" y="2" z="5" w="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.4" g="0.4" b="0.4" a="1.0" />
            <specular r="0.2" g="0.2" b="0.2" a="1.0" />
            <attenuation constant="0.0" linear="0.05" quadratic="0.0" />
        </omni>

        <spot id="pieceSpotlight" angle="30" exponent="127" enabled="0" class="piece-spotlight">
            <location x="0.0" y="1.5" z="0" w="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
            <specular r="0.5" g="0.0" b="0.0" a="1.0" />
            <attenuation constant="0.0" linear="0.75" quadratic="0.0" />
            <target x="0" y="0" z="0" />
        </spot>

        <omni id="ambient" enabled="0">
            <location x="0" y="0" z="0" w="1.0" />
            <ambient r="0.25" g="0.25" b="0.25" a="1.0" />
            <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />
            <specular r="0.0" g="0.0" b="0.0" a="1.0" />
            <attenuation constant="0.0" linear="0.9" quadratic="0.0" />
        </omni>

        <spot id="Desk spotlight" enabled="1" angle="40" exponent="2">
            <location x="-15" y="17.55" z="-10" w="1.0" />
            <target x="-20" y="0" z="-15" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.4" g="0.4" b="0.4    " a="1.0" />
            <attenuation constant="0" linear="0.03" quadratic="0.0" />
        </spot>

        <omni id="Corner light" enabled="1" angle="90" exponent="128.0">
            <location x="23" y="0" z="23.5" w="1.0" />
            <ambient r="0.75" g="0.75" b="0.75" a="1.0" />
            <diffuse r="0.9" g="0.9" b="0.9" a="1.0" />
            <specular r="0.4" g="0.4" b="0.4" a="1.0" />
            <attenuation constant="0.0" linear="0.20" quadratic="0.0" />
        </omni>

        <omni id="Camo light" enabled="1" angle="90" exponent="128.0">
            <location x="-23" y="1.5" z="15.0" w="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.9" g="0.9" b="0.9" a="1.0" />
            <specular r="0.9" g="0.9" b="0.9" a="1.0" />
            <attenuation constant="0.00" linear="0.05" quadratic="0.0" />
        </omni>
    </lights>

    <textures>
        <texture id="camoTexture" file="scenes/images/camo.jpg" />
    </textures>

    <materials>
        <material id="demoMaterial" shininess="5">
            <emission r="0.2" g="0.2" b="0.2" a="1.0" />
            <ambient r="0.5" g="0.5" b="0.9" a="1.0" />
            <diffuse r="0.5" g="0.5" b="0.9" a="1.0" />
            <specular r="0.1" g="0.1" b="0.9" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="demoTransform">
            <scale x="5" y="1" z="5" />
            <rotate axis="x" angle="-45" />
            <rotate axis="z" angle="-45" />
        </transformation>
    </transformations>

    <animations>

        <keyframeanim id="button-click" animation-speed="1.0" loop="false">
            <keyframe instant="0.0">
                <translation x="0" y="0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="0.1" >
                <translation x="0" y="0" z="-0.0175" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1.0" />
            </keyframe>
            <keyframe instant="0.2" >
                <translation x="0" y="0.0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>
        </keyframeanim>

        <keyframeanim id="piece-move" animation-speed="1.0" loop="false">
            <param name="posx" />
            <param name="posz" />
            <keyframe instant="0.0">
                <translation x="0" y="0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="0.1">
                <translation x="0" y="0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="0.50" y="1.5" z="1.0" />
            </keyframe>
            <keyframe instant="1.0" function="cubic-bezier_0.28_1.17_1_1.0">
                <translation x="posx" y="0.0" z="posz" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>
        </keyframeanim>
        <keyframeanim id="message" animation-speed="1.0" loop="true">
            <param name="x" />
            <param name="y" />
            <param name="y2" />
            <param name="z" />
            <param name="ry" />
            <keyframe instant="0.0">
                <translation x="x" y="y" z="z" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="ry" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="0.5">
                <translation x="x" y="y2" z="z" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="ry" />
                <rotation axis="x" angle="0" />
                <scale x="0.95" y="1.05" z="1.0" />
            </keyframe>
            <keyframe instant="1.0" function="cubic-bezier_0.28_1.17_1_1.0">
                <translation x="x" y="y" z="z" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="ry" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>
        </keyframeanim>

        <keyframeanim id="piece-storage_xz" animation-speed="0.75" loop="false">
            <param name="posx" />
            <param name="posz" />
            <keyframe instant="0.0">
                <translation x="0" y="0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="0.2">
                <translation x="0.250" y="0.0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.25" y="0.5" z="1.25" />
            </keyframe>
            <keyframe instant="0.3">
                <translation x="0.250" y="0.0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="0.75" y="1.5" z="0.75" />
            </keyframe>
            <keyframe instant="1.3">
                <translation x="posx" y="0" z="posz" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>

        </keyframeanim>
        <keyframeanim id="piece-storage_y" animation-speed="0.75" loop="false">
            <param name="posy" />
            <param name="posy_half" />
            <keyframe instant="0.0">
                <translation x="0" y="0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="0.2">
                <translation x="0.0" y="0.0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1" />
            </keyframe>
            <keyframe instant="0.3">
                <translation x="0.0" y="0.03" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>
            <keyframe instant="0.8" function="quadratic_0_1_1_0">
                <translation x="0" y="posy_half" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>
            <keyframe instant="1.3" function="quadratic_0_0_-1_0">
                <translation x="0" y="posy" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>

            <keyframe instant="1.4">
                <translation x="0" y="posy" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.25" y="0.75" z="1.25" />
            </keyframe>

            <keyframe instant="1.5" function="quadratic_0_0_-1_0">
                <translation x="0" y="posy" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="0.75" y="1.5" z="0.75" />
            </keyframe>
            <keyframe instant="1.6" function="quadratic_0_0_-1_0">
                <translation x="0" y="posy" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.0" y="1.0" z="1.0" />
            </keyframe>
        </keyframeanim>

        <keyframeanim id="piece-selected" animation-speed="2" loop="true">
            <keyframe instant="0.0">
                <translation x="0" y="0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
            <keyframe instant="0.2">
                <translation x="0" y="-0.015" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1.25" y="0.5" z="1.25" />
            </keyframe>
            <keyframe instant="0.4">
                <translation x="0" y="0.03" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="0.75" y="1.5" z="0.75" />
            </keyframe>

            <keyframe instant="0.9" function="quadratic_0_1_1_0">
                <translation x="0" y="0.1" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>

            <keyframe instant="1.6" function="quadratic_0_0_-1_0">
                <translation x="0" y="0.03" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="0.75" y="1.5" z="0.75" />
            </keyframe>

            <keyframe instant="1.8">
                <translation x="0" y="0" z="0" />
                <rotation axis="z" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="x" angle="0" />
                <scale x="1" y="1" z="1" />
            </keyframe>
        </keyframeanim>

    </animations>

    <primitives>
        <primitive id="rectangle">
            <rectangle x1="-0.5" y1="-1" x2="0.5" y2="1" />
        </primitive>
        <primitive id="tentOuter">
            <patch degree_u="1" parts_u="10" degree_v="2" parts_v="10">
                <controlpoint x="2" y="0" z="-2" />
                <controlpoint x="2" y="5" z="0" />
                <controlpoint x="2" y="0" z="2" />

                <controlpoint x="-2" y="0" z="-2" />
                <controlpoint x="-2" y="5" z="0" />
                <controlpoint x="-2" y="0" z="2" />
            </patch>
        </primitive>
        <primitive id="tentInner">
            <patch degree_u="1" parts_u="10" degree_v="2" parts_v="10">
                <controlpoint x="-2" y="0" z="-2" />
                <controlpoint x="-2" y="5" z="0" />
                <controlpoint x="-2" y="0" z="2" />

                <controlpoint x="2" y="0" z="-2" />
                <controlpoint x="2" y="5" z="0" />
                <controlpoint x="2" y="0" z="2" />
            </patch>
        </primitive>
    </primitives>

    <models>
        <model id="boardModel" file="board/board.obj" class="board-model" />
        <model id="whiteModel" file="piece/white.obj" class="white-piece" />
        <model id="blackModel" file="piece/black.obj" class="black-piece" />
        <model id="blackStorage" file="storage/black_storage.obj" class="black-storage" />
        <model id="whiteStorage" file="storage/white_storage.obj" class="white-storage" />
        <model id="gameClock" file="clock/game_clock.obj" class="game-clock" />
        <model id="whiteCounter" file="clock/white_counter.obj" class="white-counter" />
        <model id="blackCounter" file="clock/black_counter.obj" class="black-counter" />
        <model id="button" file="button/button.obj" class="button" />
    </models>

    <components>
        <component id="demoRoot">
            <transformation></transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="none" />
            <children>
                <componentref id="board" />
                <componentref id="white_storage" />
                <componentref id="black_storage" />
                <componentref id="game-clock-global" />
                <componentref id="game-clock-white" />
                <componentref id="game-clock-black" />
                <componentref id="white-counter" />
                <componentref id="black-counter" />
                <componentref id="help-text" />
                <componentref id="single-clock-black" />
                <componentref id="single-clock-white" />
                <componentref id="undo-white" />
                <componentref id="undo-black" />
                <componentref id="start-white" />
                <componentref id="start-black" />
                <componentref id="view-white"/>
                <componentref id="view-black"/>
            </children>
        </component>
        <component id="board" class="board" pickable="true">
            <transformation>
                <translate x="0.0" y="0.0" z="0.0" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" />
            <children>
                <modelref id="boardModel" />

            </children>
        </component>
        <component id="white" class="white-piece" pickable="true">
            <transformation>
                <translate x="-0.0" y="0.15" z="0.0" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" />
            <children>
                <modelref id="whiteModel" />

            </children>
        </component>

        <component id="black" class="black-piece" pickable="true">
            <transformation>
                <translate x="0.0" y="0.15" z="0.0" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" />
            <children>
                <modelref id="blackModel" />

            </children>
        </component>

        <component id="black_storage" class="black-storage">
            <transformation>
                <translate x="0.0" y="0.0" z="-2.0" />
                <rotate axis="y" angle="180" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit" />
            <children>
                <modelref id="blackStorage" />
            </children>
        </component>

        <component id="white_storage" class="white-storage">
            <transformation>
                <translate x="0.0" y="0.0" z="2.0" />
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="none" />
            <children>
                <modelref id="whiteStorage" />
            </children>
        </component>


        <component id="black-counter">
